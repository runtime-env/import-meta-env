{
  "name": "@import-meta-env/swc",
  "version": "0.4.4",
  "description": "Build once, deploy anywhere. Startup/runtime environment variable solution for JavaScript.",
  "license": "MIT",
  "author": "Ernest",
  "keywords": [
    "swc-plugin"
  ],
  "main": "import_meta_env_swc.wasm",
  "scripts": {
    "prepare": "rustup target add wasm32-wasi",
    "build": "cargo build --all --release --target wasm32-wasi",
    "prepack": "cp target/wasm32-wasi/release/import_meta_env_swc.wasm .",
    "pack": "rm -rf *.tgz && pnpm pack && node ../../scripts/rename-tgz.js",
    "release": "standard-version --skip.tag -t swc --preset=conventionalcommits --releaseCommitMessageFormat 'chore(release): @import-meta-env/swc@{{currentTag}}' --path .",
    "test": "cargo test --all --color always -- --test-threads 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iendeavor/import-meta-env.git",
    "directory": "packages/swc"
  },
  "bugs": {
    "url": "https://github.com/iendeavor/import-meta-env/issues"
  },
  "homepage": "https://github.com/iendeavor/import-meta-env/tree/main/packages/swc#readme",
  "peerDependencies": {
    "@import-meta-env/cli": "^0.5.1",
    "@swc/core": "^1.3.38"
  },
  "peerDependenciesMeta": {
    "@import-meta-env/cli": {
      "optional": true
    },
    "@swc/core": {
      "optional": true
    }
  }
}
