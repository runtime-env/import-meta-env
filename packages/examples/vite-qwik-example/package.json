{
  "name": "vite-qwik-example",
  "description": "Recommended for your first Qwik app (comes with Qwik City)",
  "engines": {
    "node": ">=15.0.0"
  },
  "private": true,
  "scripts": {
    "_test": "node test.js",
    "build": "qwik build",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.types": "tsc --incremental --noEmit",
    "dev": "cross-env HELLO=import-meta-env vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint src/**/*.ts*",
    "preview": "qwik build preview && cross-env HELLO=import-meta-env import-meta-env -x .env.example.public -p \"server/**/*\" \"dist/**/*\" && vite preview --open",
    "start": "vite --open --mode ssr",
    "qwik": "qwik"
  },
  "devDependencies": {
    "@builder.io/qwik": "^0.107.0",
    "@builder.io/qwik-city": "^0.107.0",
    "@types/eslint": "8.56.12",
    "@types/node": "latest",
    "@typescript-eslint/eslint-plugin": "5.37.0",
    "@typescript-eslint/parser": "5.37.0",
    "cross-env": "^7.0.3",
    "eslint": "8.57.1",
    "eslint-plugin-qwik": "^0.107.0",
    "node-fetch": "3.2.10",
    "prettier": "2.7.1",
    "typescript": "4.8.3",
    "vite": "^3.2.2",
    "vite-tsconfig-paths": "3.5.0"
  }
}
