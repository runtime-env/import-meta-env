{
  "name": "svelte-kit-example",
  "private": true,
  "scripts": {
    "dev": "cross-env HELLO=import-meta-env svelte-kit dev",
    "build": "svelte-kit build",
    "package": "svelte-kit package",
    "preview": "cross-env HELLO=import-meta-env import-meta-env -x .env.example.public -o \".svelte-kit/output/**\" && svelte-kit preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch"
  },
  "devDependencies": {
    "@import-meta-env/cli": "workspace:^0.1.4",
    "@import-meta-env/unplugin": "workspace:^0.1.4",
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/kit": "next",
    "@types/cookie": "^0.4.1",
    "cross-env": "^7.0.3",
    "svelte": "^3.46.0",
    "svelte-check": "^2.2.6",
    "svelte-preprocess": "^4.10.1",
    "tslib": "^2.3.1",
    "typescript": "~4.6.2"
  },
  "type": "module",
  "dependencies": {
    "@fontsource/fira-mono": "^4.5.0",
    "@lukeed/uuid": "^2.0.0",
    "cookie": "^0.4.1"
  }
}
